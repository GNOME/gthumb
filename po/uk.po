#: data/gthumb.schemas.in.h:1
msgid ""
"Images over this size (in bytes) will not be thumbnailed. Use 0 if you want "
"to generate thumbnails for all images."
msgstr ""
"З зображень цього розміру (у байтах) не будуть створюватись мініатюри. Вкажіть 0, "
"якщо бажаєте створювати мініатюри для будь-яких зображень."

#: data/gthumb.schemas.in.h:2
msgid "Possible values are: 1, 2, 3, 4."
msgstr "Можливі значення: 1, 2, 3, 4."

#: data/gthumb.schemas.in.h:3
msgid "Possible values are: actual_size, fit, keep_prev, fit_if_larger."
msgstr "Можливі значення: actual_size(не масштабувати), fit(масштабувати), keep_prev(попередній масштаб), fit_if_larger(зменшувати великі)."

#: data/gthumb.schemas.in.h:4
msgid "Possible values are: ascending, descending."
msgstr "Можливі значення: ascending(зростаючий), descending(спадаючий)."

#: data/gthumb.schemas.in.h:5
msgid "Possible values are: forward, backward, random."
msgstr "Можливі значення: forward(прямий), backward(зворотний), random(випадково)."

#: data/gthumb.schemas.in.h:6
msgid "Possible values are: high, low."
msgstr "Можливі значення: high(висока), low(низька)."

#: data/gthumb.schemas.in.h:7
msgid "Possible values are: image, data, comment."
msgstr "Можливі значення: image(зображення), data(дані), comment(коментарі)."

#: data/gthumb.schemas.in.h:8
msgid "Possible values are: jpeg, png, tga, tiff."
msgstr "Можливі значення: jpeg, png, tga, tiff."

#: data/gthumb.schemas.in.h:9
msgid "Possible values are: light, midtone, dark."
msgstr "Можливі значення: light(світлий), midtone(півтони), dark(темний)."

#: data/gthumb.schemas.in.h:10
msgid "Possible values are: list, thumbnails."
msgstr "Можливі значення: list(список), thumbnails(мініатюри)."

#: data/gthumb.schemas.in.h:11
msgid "Possible values are: mm, in."
msgstr "Можливі значення: mm(міліметри), in(дюйми)."

#: data/gthumb.schemas.in.h:12
msgid "Possible values are: name, path, size, time."
msgstr "Можливі значення: name(назва), path(шлях), size(розмір), time(час)."

#: data/gthumb.schemas.in.h:13
msgid "Possible values are: name, size, time."
msgstr "Можливі значення: name(назва), size(розмір), time(час)."

#: data/gthumb.schemas.in.h:14
msgid "Possible values are: nautilus, single, double."
msgstr "Можливі значення: nautilus(як у Наутілусі), single(одиночним), double(подвійним)."

#: data/gthumb.schemas.in.h:15
msgid "Possible values are: none, deflate, jpeg."
msgstr "Можливі значення: none(без стиснення), deflate(стискати без втрат), jpeg."

#: data/gthumb.schemas.in.h:16
msgid "Possible values are: none, name, path, size, time."
msgstr "Можливі значення:  none(не впорядковувати), name(за назвою), path(за шляхом), size(за розміром), time(за часом)."

#: data/gthumb.schemas.in.h:17
msgid ""
"Possible values are: none, simple, simple_with_shadow, shadow, slide, "
"shadow_in, shadow_out."
msgstr "Можливі значення: none(без рамки), simple(рамка), simple_with_shadow(рамка з тінню), shadow(лише тінь), slide(слайд), "
"shadow_in(витиснутий), shadow_out(випуклий)."

#: data/gthumb.schemas.in.h:18
msgid ""
"Possible values are: none, square, image, display, 4x3, 4x6, 5x7, 8x10, "
"custom."
msgstr "Можливі значення: none(немає), square(квадрат), image(зображення), display(дисплей), 4x3, 4x6, 5x7, 8x10, "

#: data/gthumb.schemas.in.h:19
msgid "Possible values are: pixels, percentage."
msgstr "Можливі значення: pixels(точок), percentage(відсоток)."

#: data/gthumb.schemas.in.h:20
msgid "Possible values are: png, jpeg."
msgstr "Можливі значення: png, jpeg."

#: data/gthumb.schemas.in.h:21
msgid "Possible values are: skip, rename, ask, overwrite."
msgstr "Можливі значення: skip(пропускати), rename(перейменовувати), ask(питати), overwrite(переписувати)."

#: data/gthumb.schemas.in.h:22
msgid "Possible values are: small, medium, large."
msgstr "Можливі значення: small(маленький), medium(середній), large(великий)."

#: data/gthumb.schemas.in.h:23
msgid ""
"Possible values are: system, text_below, text_beside, text_only, icons_only."
msgstr "Можливі значення: system(як у системі), text_below(текст під значками), text_beside(текст поруч), text_only(лише текст), icons_only(лише значки)."

#: data/gthumb.schemas.in.h:24
msgid "Possible values are: white, black, checked, none."
msgstr "Можливі значення: white(білий), black(чорний), checked(клітинки), none(немає)."

#: data/gthumb.schemas.in.h:25
